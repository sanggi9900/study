package base3;

public class OperatorEX2 {
	public static void main(String[] args) {
		// 실습2
		// - 실수값을 하나만 입력을 받아서
		// 정수부와 실수부를 나눠서 결과예시처럼 출력하세요.
		
		// 결과예시(3.98 입력시)
		// 정수부 : 3
		// 실수부 : 0.98
		// # 실수부는 소수점이 더 많이 나올 수 있고, 그대로 출력하시면 됩니다.
		// # 정수 -> 실수 형변환 : 3 -> 3.0
		// # 실수 -> 정수 형변환 : 3.9999999 -> 3
		Scanner sc = new Scanner(System.in);
	}
}
