package main;

public class FinalEX {
	public static void main(String[] args) {
		Playable pc1 = new Playable("마을주민", 100);
		Playable pc2 = new Warrior("전사", 200);
		Playable pc3 = new Wizard("마법사", 100, 200);
		Playable pc4 = new Healer("신관", 50, 300);
		pc1.status();
		pc2.status();
		pc3.status();
		pc4.status();
	}
}
