package part3;

public class Overload1 {
	public static int getExpo1(int n1, int n2) {
		if (n2 < 0) return 0;
		else {
			int result = 1;
			while (n2 > 0) {
				result *= n1;
				n2 -= 1;
			}
			return result;
		}
	}
	public static double getExpo2(double n1, int n2) {
		if (n2 < 0) return 0;
		else {
			double result = 1;
			while (n2 > 0) {
				result *= n1;
				n2 -= 1;
			}
			return result;
		}
	}
}
