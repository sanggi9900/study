package whilestate;

public class While2 {
	public static void main(String[] args) {
		// 문자열 "같은 것들" 의 비교 : 요소(item)을 모두 비교해줘야 함
		String word1 = "ABC";
		String word2 = "ABE";
		// 문자열의 비교는 인덱스를 이용해 같은 위치를 지정해서 비교해야 함
		while ()
		System.out.println(word1.charAt(0) == word2.charAt(0));
		System.out.println(word1.charAt(1) == word2.charAt(1));
		System.out.println(word1.charAt(2) == word2.charAt(2));
	}
}
