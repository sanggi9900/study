package classSource;

public class Human {
	private String name;
	private String roll;
	private int age;
	
	public void setName(String name) {
		this.name = name;
	}
	public void setRoll(String roll) {
		this.roll = roll;
	}
	public void setAge(int age) {
		this.age = age;
	}
	
	public String getName() {
		return name;
	}
	public String getRoll() {
		return roll;
	}
	public int getAge() {
		return age;
	}
	
	public void greeting() {
		
	}
	public void action() {
		
	}
}
