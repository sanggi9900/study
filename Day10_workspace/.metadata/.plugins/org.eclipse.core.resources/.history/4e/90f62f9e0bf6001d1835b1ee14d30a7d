package part1;

public class ArrayEX1 {
	public static void main(String[] args) {
		int num1 = 10;
		int num2 = 20;
		int num3 = 30;
		int[] arr = new int[3];
		System.out.printf("index : 0, value : %d\n", num1);
		System.out.printf("index : 1, value : %d\n", num2);
		System.out.printf("index : 2, value : %d\n", num3);
	}
}
