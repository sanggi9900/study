package part1;

public class ArrayEX1 {
	public static void main(String[] args) {
		int[] arr = new int[3];
		arr[0] = 10;
		arr[1] = 20;
		arr[2] = 30;
		// int[] arr = { 10, 20, 30 };
		for (int i = 0; i < 3 ; i += 1) {
			System.out.printf("index : %d, value : %d\n", i, arr[0]);
		}
		
		System.out.printf("index : 1, value : %d\n", arr[1]);
		System.out.printf("index : 2, value : %d\n", arr[2]);
	}
}
