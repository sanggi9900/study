package part2;

public class MethodEX3 {
	// 매개변수가 있는 메서드를 준비할 때 주의할 점
	// - 적정수량의 매개변수만 준비하는게 좋음(최대 5개, 보통 3개 이하)
	public static void showMax(int n1, int n2, int n3) {
		if (n1 == n2 && n2 == n3) System.out.println("전부 같습니다.");
		else {
			
		}
	}
	public static void main(String[] args) {
		showMax(1,2,3);
		showMax(3,2,1);
		showMax(1,3,2);
		showMax(2,2,3);
		showMax(3,2,3);
		showMax(3,3,3);
	}
}
