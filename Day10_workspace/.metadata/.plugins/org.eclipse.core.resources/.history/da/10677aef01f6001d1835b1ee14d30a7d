package part3;

public class OverloadEX3 {
	// 오버로딩은 매개변수까지 메서드를 구분하는 용도로 쓰기에
	// 같은 이름에 같은 매개변수로 호출하는게 아니면 서로 다른
	// 메서드로 구분되어 인식됨
	
	
	public static void showNumber(int n1, int n2, int n3) {}
	
	public static void main(String[] args) {
		showNumber(6);
		showNumber(2,7);
		showNumber(1,12,2);
	}
}
