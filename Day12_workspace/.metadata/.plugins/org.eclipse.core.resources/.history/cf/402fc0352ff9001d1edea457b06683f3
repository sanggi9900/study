package runSpace;

public class BASE2_EX {
	public static void main(String[] args) {
		// 실습
		// 1) Box2 클래스의 필드는 모두 public입니다.
		//  접근제어를 설정해서 사용을 통제하세요.
		
		// 2) Box2 클래스의 필드에 대한 setter와 getter를
		//   정의하고 이를 이용하여 값을 저장하고 출력하세요.
		Box2 box2 = new Box2();
		box2.setName("접근제어가 적용된 상자");
		box2.setState(false);
		System.out.println(
				box2.getName()
				+ " " +
				box2.getState()
				);
		box2.open();
		System.out.println(box2.getName() + " " + box2.getState());
	}
}
